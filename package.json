{
  "name": "@foxkey/express-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Davilson de Castro",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prestart": "yarn install",
    "start": "dotenv -e .env -- ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "dev": "yarn start",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "preserve": "yarn build",
    "serve": "dotenv -e .env -- node ./dist/server.js",
    "lint": "eslint ./",
    "lintFix": "eslint ./ --fix",
    "test:functional": "dotenv -e .env -- jest --projects ./__test__ --runInBand",
    "test:unit": "dotenv -e .env -- jest",
    "test": "yarn test:unit && yarn test:functional"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pino": "^7.0.2",
    "pino-pretty": "^7.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
